{
    "name": "recca0120/laravel-bridge",
    "description": "use laravel eloquent, view, pagination anywhere",
    "keywords": ["laravel", "artisan", "terminal", "console", "web", "web artisan"],
    "license": "MIT",
    "authors": [
        {
            "name": "recca0120",
            "email": "recca0120@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/config": "^5.2",
        "illuminate/container": "^5.2",
        "illuminate/database": "^5.2",
        "illuminate/events": "^5.2",
        "illuminate/http": "^5.2",
        "illuminate/pagination": "^5.2",
        "illuminate/view": "^5.2",
        "recca0120/laravel-tracy": "^1.5"
    },
    "require-dev": {
        "mockery/mockery": "~0.9.2",
        "nesbot/carbon": "~1.20",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^4 | ^5 | ^6",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "conflict": {
        "illuminate/notifications": "5.3.* | 5.4.*",
        "illuminate/pagination": "5.3.* | 5.4.*"
    },
    "autoload": {
        "psr-4": {
            "Recca0120\\LaravelBridge\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
