{
    "name": "recca0120/laravel-bridge",
    "description": "use laravel eloquent, view, pagination anywhere",
    "keywords": ["laravel", "artisan", "terminal", "console", "web", "web artisan"],
    "license": "MIT",
    "authors": [
        {
            "name": "recca0120",
            "email": "recca0120@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/view": "5.2.*",
        "illuminate/database": "5.2.*",
        "illuminate/events": "5.2.*",
        "illuminate/pagination": "5.2.*",
        "illuminate/http": "5.2.*",
        "recca0120/laravel-tracy": "1.5.*"
    },
    "require-dev": {
        "nesbot/carbon": "~1.20",
        "mockery/mockery": "~0.9.2",
        "phpunit/phpunit": "~4.1"
    },
    "autoload": {
        "psr-4": {
            "Recca0120\\LaravelBridge\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
